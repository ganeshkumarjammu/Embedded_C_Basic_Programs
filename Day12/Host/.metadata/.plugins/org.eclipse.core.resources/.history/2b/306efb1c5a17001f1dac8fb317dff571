	uint32_t carPrice;
//	uint8_t carMaxSpeed;
	 float carWeight;
};

int main(){
	struct carModel BMW = {2000,400.5};
	printf("Size of structure variable %u\n",sizeof(BMW));
	return 0 ;
}

