int main(void)
{
#include <stdio.h>
#include "math.h"

int main() {

    float a = 10.0f, b = 5.0f;
    float result;
    printf("Enter Number1 , Number2 :");
    fflush(stdout);
    scanf("%f %f ",&a,&b);
    result = add(a, b);
    printf("Result of addition: %f\n", result);

    result = subtract(a, b);
    printf("Result of subtraction: %f\n", result);

    result = multiply(a, b);
    printf("Result of multiplication: %f\n", result);

    result = divide(a, b);
    printf("Result of division: %f\n", result);

    return 0;
}

    /* Loop forever */
	for(;;);
}
