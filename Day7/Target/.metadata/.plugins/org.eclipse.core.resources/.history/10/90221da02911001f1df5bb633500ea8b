#include "math.h"
#include <stdio.h>

float add(float a, float b) {
    printf("Adding %f and %f\n", a, b);
    return a + b;
}

float subtract(float a, float b) {
    printf("Subtracting %f from %f\n", b, a);
    return a - b;
}

float multiply(float a, float b) {
    printf("Multiplying %f and %f\n", a, b);
    return a * b;
}

float divide(float a, float b) {
    printf("Dividing %f by %f\n", a, b);
    return a / b;
}
